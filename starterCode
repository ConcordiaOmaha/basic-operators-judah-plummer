
// Example program
#include <iostream>
#include <string>

int main()
{

  int num1 = 0; 
  int num2 = 0;
  std::cout << "enter a number: "; 
  std::cin >> num1; 
  std::cout << "enter a second number: ";
  std::cin  >> num2;
  int num3 = num2 + num1; 
  std::cout << "Your answer is: " << num3;
    
}
